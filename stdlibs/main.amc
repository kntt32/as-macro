import std;
import std.io;
import std.string;

fn func_test(in value: u64@stack) {
    printfln(value);
};

fn main() {
    let optional_stream: Option_In@stack;
    let stream: defer In@stack;
    let str: Str@stack;
    optional_stream.open(str.from("main.amc"));
    optional_stream.take(stream);

    let buff: defer String@stack;
    stream.read(buff);

    printfln(buff);

    let value: u64@stack = 1;
    func_test(value);
};

