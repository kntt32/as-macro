import core;

extern fn panic_handler();

pub as panic() {
    panic_handler();
    ud2();
};

pub as panic_eq(in left: i64@reg+mem, in right: i64@reg+mem+imm) {
    let flag: bool@rax;
    if(flag.eq(left, right)) {
        panic();
    };
};

pub as panic_le(in left: i64@reg+mem, in right: i64@reg+mem+imm) {
    let flag: bool@rax;
    if(flag.le(left, right)) {
        panic();
    };
};

