import core_x64;

pub const stdin: i64 = 0;
pub const stdout: i64 = 1;
pub const stderr: i64 = 2;

pub as write(fd: i64@rdi, msg: char*@rsi, len: i64@rdx) {
    let number: i64@rax;
    number = 1;
    syscall();
};

pub as exit(status: i32@rdi) {
    let number: i64@rax;
    number = 60;
    syscall();
};

