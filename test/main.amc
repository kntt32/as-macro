import std;
import std.io;
import core.cmp;

type MyIo = Io;

fn io_test() {
    let io: MyIo@stack;
    io.new(stdout);

    let msg: *char@rsi;
    msg =& "Hello, World!!\n";
    
    let len: i64@rcx;
    len.strlen(msg);

    let data: *i8@stack;
    data = msg;
    for((let i: i64@r9=0;let flag: bool@rax;); flag.lt(i, 2); i+=1) {
        io.write(data, len);
    };
    io.flush();

};

template MyTemp(T) {
    as mytemp_macro_$(T)() {
        io_test();
    };
};

impl MyTemp(i32);

fn main() {
    let fnptr: fn()@rax;
    fnptr =& io_test;
    io_test();
    (fnptr)();
    mytemp_macro_i32();
};



