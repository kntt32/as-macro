import core_x64;

struct Point {
    x: i64,
    y: i64
};

pub as mov(dst: Point@mem, src: Point@mem) {
    push(rax);
    let a: i64@rax;
    a = src.x;
    dst.x = a;
    a = src.y;
    dst.y = a;
    pop(rax);
};

fn addptr(val: i64@rax, ptr: Point@stack) {
    val = ptr.x;
    add(val, ptr.y);
};

fn test() {
