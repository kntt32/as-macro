import std;
import std.io;
import core.cmp;

pub fn main() {
    let io: Io@stack;
    new(io, stdout);
    let msg: *char@rsi;
    msg =& "Hello, World!!\n";
    let len: i64@rcx;
    strlen(len, msg);
    let data: *i8@stack;
    data = msg;
    write(io, data, len);
    flush(io);

