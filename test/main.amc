import std;
import std.io;
import core.cmp;

static string: [char; 20] = "Hello, World!\n";

pub fn main() {
    let flag: bool@r10;
    let i: i64@r11;
    for(i=0; lt(flag, i, 5); inc(i)) {
        let fd: i64@rdi;
        let msg: *char@rsi;
        let len: i64@rdx;

        fd = stdout;
        lea(msg, string);
        let strlen: i64@rcx;
        len = strlen(strlen, msg);

        write(fd, msg, len);
    };
};



