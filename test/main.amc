import std;
import std.io;
import core.cmp;

static string: [char; 20] = "Hello, World!\n";

pub fn main() {
    for((let i: i64@r11; let flag: bool@r10; i=0;); lt(flag, i, 5); inc(i)) {
        let fd: i64@rdi = stdout;
        let msg: *char@rsi;
        msg =& string;

        let strlen: i64@rcx;
        let len: i64@rdx = strlen(strlen, msg);

        write(fd, msg, len);
    };
};



