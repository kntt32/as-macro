import std;
import std.io;

static string: [char; 20] = "Hello, World!\n";
static string2: [char; 20] = "hello, world\n";

pub fn main() {
    let sbuff: [char; 100]@stack;
    let dst: *char@rdi;
    let src: *char@rsi;
    strcpy(lea(dst, sbuff), lea(src, string));

    let fd: i64@rdi;
    let msg: *char@rsi;
    let len: i64@rdx;

    let flag: bool@rax;
    flag = true;
    fd = stdout;
    
    lea(msg, sbuff);
    let strlen: i64@rcx;
    len = strlen(strlen, msg);

    write(fd, msg, len);
};



